<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add key="RunAsLocalService" value="true" xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>
  </appSettings>
  <WebServiceSettings xdt:Transform="Remove"></WebServiceSettings>
  <connectionStrings xdt:Transform="Remove"></connectionStrings>
  <system.net>
    <mailSettings xdt:Transform="Remove"></mailSettings>
  </system.net>
  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
  </system.web>
</configuration>